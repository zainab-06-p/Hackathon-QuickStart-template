    "contract": {
        "name": "Ticketing",
        "desc": "\n    CampusChain Ticketing Contract\n    NFT-based event tickets with anti-scalping and entry verification\n    Each deployment = one event (fully decentralized)\n    Features:\n    - Mints NFT ticket on purchase\n    - QR code verification at entry\n    - Prevents double entry\n    ",
        "methods": [
            {
                "name": "create_event",
                "args": [
                    {
                        "type": "uint64",
                        "name": "price"
                    },
                    {
                        "type": "uint64",
                        "name": "supply"
                    },
                    {
                        "type": "uint64",
                        "name": "event_date"
                    },
                    {
                        "type": "uint64",
                        "name": "sale_end_date"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "uint64"
                },
                "desc": "Create a new ticketed event (called during contract creation)"
            },
            {
                "name": "buy_ticket",
                "args": [
                    {
                        "type": "pay",
                        "name": "payment"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "uint64"
                },
                "desc": "Purchase an event ticket - Mints NFT to buyer\nReturns the NFT Asset ID"
            },
            {
                "name": "verify_entry",
                "args": [
                    {
                        "type": "account",
                        "name": "ticket_holder"
                    },
                    {
                        "type": "uint64",
                        "name": "ticket_asset_id"
                    }
                ],
                "readonly": false,
                "returns": {
                    "type": "bool"
                },
                "desc": "Verify ticket at event entry (called by organizer/staff)\nChecks NFT ownership and check-in status"
            },
            {
                "name": "is_checked_in",
                "args": [
                    {
                        "type": "uint64",
                        "name": "ticket_asset_id"
                    }
                ],
                "readonly": true,
                "returns": {
                    "type": "bool"
                },
                "desc": "Check if a ticket has been used for entry"
            },
            {
                "name": "toggle_sale",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "bool"
                },
                "desc": "Toggle ticket sales on/off (organizer only)"
            },
            {
                "name": "get_event_info",
                "args": [],
                "readonly": false,
                "returns": {
                    "type": "(uint64,uint64,uint64,uint64,uint64,uint64,bool)"
                },
                "desc": "Get event information"
            },
            {
                "name": "get_organizer",
                "args": [],
                "readonly": true,
                "returns": {
                    "type": "address"
                },
                "desc": "Get event organizer address"
            }
        ],
